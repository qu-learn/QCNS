    "test:qasm": "node test/run_qasm_tests.js",
    "test:examples": "node examples/basic_usage.js && node examples/qasm_examples.js",
    "test:all": "npm run test && npm run test:examples",
    "build": "echo 'Build script not yet implemented'",
    "dev": "echo 'Development server not yet implemented'",
    "lint": "echo 'Linting not yet configured'"
  },
  "keywords": [
    "quantum",
    "circuit",
    "simulation",
    "qiskit",
    "quantum-computing",
    "quantum-simulator",
    "javascript",
    "es6",
    "quantum-gates",
    "quantum-network",
    "openqasm",
    "qasm",
    "transpiler",
    "quantum-transpiler"
  ],
  "author": "QCNS Development Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/qcns.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/qcns/issues"
  },
  "homepage": "https://github.com/your-org/qcns#readme",
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "src/",
    "ui/",
    "examples/",
    "test/",
    "docs/",
    "README.md",
    "DOCUMENTATION.md",
    "CHANGELOG.md",
    "CONTRIBUTING.md",
    "LICENSE",
    "styles.css"
  ],
  "exports": {
    ".": {
      "import": "./src/index.js",
      "require": "./src/index.js"
    },
    "./core": {
      "import": "./src/core/QuantumCircuit.js"
    },
    "./registers": {
      "import": "./src/core/QuantumRegister.js"
    },
    "./gates": {
      "import": "./src/core/QuantumGates.js"
    },
    "./math": {
      "import": "./src/core/ComplexMath.js"
    }
