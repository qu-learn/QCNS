{
  "name": "qcns",
  "version": "2.0.0",
  "description": "Quantum Circuit and Network Simulator v2.0 - A comprehensive JavaScript platform for quantum circuit simulation with modular architecture, visual network editing, and Qiskit-like API",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "test": "node test/test.js && node test/run_qasm_tests.js",
